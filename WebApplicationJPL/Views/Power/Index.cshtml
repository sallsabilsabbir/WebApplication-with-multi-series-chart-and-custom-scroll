<div class="container m-0 p-0">
    <div class="row">
        <div class="col-6 text-center">
            <p>
                <a class="btn btn-primary " data-bs-toggle="collapse" href="#collapsemaxAcPower" role="button" aria-expanded="false" aria-controls="collapseExample">
                    Max Ac Power
                </a>
            </p>
            <div class="collapse text-center" id="collapsemaxAcPower">

               @* <h5 class="text-center text-black border">Max Ac Power</h5>*@
               <style>

                    tr > th:first-child, tr > td:first-child {
                        position: sticky;
                        left: 0;
                        background-color:aliceblue;
                        opacity: .8;
                    }
               </style>

                <table class="table table-striped table-responsive" id="table_id-2">
                    <thead>
                        <tr>
                            <th>  Time </th>
                            <th>  Shed 1 </th>
                            <th>  Shed 2 </th>
                            <th>  Shed 3&4</th>
                            <th>  Shed 5</th>
                            <th>  Shed 6</th>
                            <th>  Shed 7</th>
                            <th>  Shed 8</th>
                            <th>  Shed 10</th>
                            <th>  Shed 11</th>
                            <th>  Shed 12</th>
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var maxAcPower in Model.MaxAcPowers)
                        {
                            <tr>
                               @* <td> @maxAcPower.Time </td>*@
                                <td>@Html.DisplayFor(modelItem=>maxAcPower.Time)</td>
                                <td> @maxAcPower.Shed1 </td>
                                <td> @maxAcPower.Shed2 </td>
                                <td> @maxAcPower.Shed3And4 </td>
                                <td> @maxAcPower.Shed5 </td>
                                <td> @maxAcPower.Shed6 </td>
                                <td> @maxAcPower.Shed7 </td>
                                <td> @maxAcPower.Shed8 </td>
                                <td> @maxAcPower.Shed10 </td>
                                <td> @maxAcPower.Shed11 </td>
                                <td> @maxAcPower.Shed12 </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>

        <br />

        <div class="col-6 text-center">
            <p>
                <a class="btn btn-primary " data-bs-toggle="collapse" href="#collapsemaxDcPower" role="button" aria-expanded="false" aria-controls="collapseExample">
                    Max Dc Power
                </a>
            </p>
            <div class="collapse" id="collapsemaxDcPower">
                <style>

                    tr > th:first-child, tr > td:first-child {
                        position: sticky;
                        left: 0;
                        background-color: aliceblue;
                        opacity: .8;
                    }
                </style>
               @* <h5 class="text-center text-black border">Max Dc Power</h5>*@

                <table class="table table-striped table-responsive" id="table_id-3">
                    <thead>
                        <tr>
                            <th>  Time </th>
                            <th>  Shed 1 </th>
                            <th>  Shed 2 </th>
                            <th>  Shed 3&4</th>
                            <th>  Shed 5</th>
                            <th>  Shed 6</th>
                            <th>  Shed 7</th>
                            <th>  Shed 8</th>
                            <th>  Shed 10</th>
                            <th>  Shed 11</th>
                            <th>  Shed 12</th>
                        </tr>
                    </thead>

                    <tbody>
                        @foreach (var maxDcPower in Model.MaxDcPowers)
                        {
                            <tr>
                                @*<td> @maxDcPower.Time </td>*@
                                <td>@Html.DisplayFor(modelItem=>maxDcPower.Time)</td>
                                <td> @maxDcPower.Shed1 </td>
                                <td> @maxDcPower.Shed2 </td>
                                <td> @maxDcPower.Shed3And4 </td>
                                <td> @maxDcPower.Shed5 </td>
                                <td> @maxDcPower.Shed6 </td>
                                <td> @maxDcPower.Shed7 </td>
                                <td> @maxDcPower.Shed8 </td>
                                <td> @maxDcPower.Shed10 </td>
                                <td> @maxDcPower.Shed11 </td>
                                <td> @maxDcPower.Shed12 </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>
</div>

